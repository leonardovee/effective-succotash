{
  "name": "effective-succotash",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc --p tsconfig.build.json",
    "dev": "sucrase-node src/main/server.ts",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "jest --passWithNoTests --silent --noStackTrace --runInBand --watch -c jest-unit-config.js",
    "test:integration": "jest --passWithNoTests --noStackTrace --runInBand --watch -c jest-integration-config.js",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --runInBand --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardovee/effective-succotash.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardovee/effective-succotash/issues"
  },
  "homepage": "https://github.com/leonardovee/effective-succotash#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "sucrase": "^3.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "engines": {
    "node": "15.14.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
